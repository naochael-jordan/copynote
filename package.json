{
  "name": "copynote",
  "version": "1.0.0",
  "description": "clipboard manager(Electron) for Mac",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron dist",
    "rebuild": "npm rebuild --runtime=electron --target=2.0.8 --disturl=https://atom.io/download/atom-shell --abi=48",
    "watch": "babel --watch --out-dir dist src",
    "package": "electron-packager dist --platform=darwin --all --out=release"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naochael-jordan/copynote.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naochael-jordan/copynote/issues"
  },
  "homepage": "https://github.com/naochael-jordan/copynote#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron": "^2.0.8",
    "electron-packager": "^12.2.0"
  }
}
